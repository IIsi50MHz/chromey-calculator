{
	"name": "Chromey Calculator",
	"version": "4.1",
	"description": "A shockingly versatile scrolling calculator for Chrome. Results are generated using Google and Wolfram|Alpha.",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"browser_action": {
		"default_icon": "icons/19-loading.png",
		"default_title": "Chromey Calculator",
		"default_popup": "popup.html"
	},
	"permissions": [
		"http://*.google.com/",
		"https://*.google.com/",
		"http://*.wolframalpha.com/"
	],
	"background_page": "background.html",
	"options_page": "options.html"
}