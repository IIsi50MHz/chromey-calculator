{
	"name": "Extend Chromey Calculator",
	"version": "0.1",
	"description": "Adds features to Chromey Caclulator.",
	"background_page": "background.html",		
	"icons": { 
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"css": ["cc_extend.css"],
		"js": ["cc_extend.js"],
		"all_frames": true		
	}],
	"permissions": [		
		"http://*/*",
		"https://*/*",
		"tabs",
		"bookmarks"
	]
}